{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.MySQL"
    ],
    "MinimumLevel": "Information", //Error, Information
    "WriteTo": [
      {
        "Name": "MySQL",
        "Args": {
          "tableName": "logs",
          "needAutoCreateTable": true
        }
      }
    ]
  }
}
